<view wx:if="{{isHide}}">
    <view wx:if="{{canIUse}}">
        <view class='header'>
            <image src='/images/wx_login.png'></image>
        </view>
        <view class='content'>
            <view>申请获取以下权限</view>
            <text>获得你的公开信息(昵称，头像等)</text>
        </view>
        <button class='bottom' type='primary' open-type="getUserInfo" lang="zh_CN" bindgetuserinfo="bindGetUserInfo">
            授权登录
        </button>
    </view>
    <view wx:else>请升级微信版本</view>
</view>
<view wx:else>
    <image src='{{src}}'></image>
    <text>{{nickname}}</text>
    <navigator class="" target="" url="/pages/history/history" hover-class="navigator-hover" open-type="navigate">
        浏览历史
    </navigator>
</view>
<Tabs></Tabs>